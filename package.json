{
  "name": "midas-web-app",
  "version": "0.0.0",
  "description": "Midas Web App is the default web application built on top of the Fineract platform and Angular framework.",
  "keywords": [
    "midas",
    "fineract",
    "angular"
  ],
  "homepage": "https://github.com/MidasHub/Midas_UI_Commercial",
  "license": "MPL-2.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod --output-hashing=none",
    "build:prod": "node --max-old-space-size=16384 ./node_modules/@angular/cli/bin/ng build --prod --output-hashing=none --base-href=\"/web-app/\"",
    "build:coreapp": "ng build --prod --output-hashing=all --serviceWorker=true --base-href=\"/coreapp/\"",
    "build:billpos": "ng build --prod --output-hashing=all --serviceWorker=true --base-href=\"/billpos/\"",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@akveo/ng2-completer": "9.0.1",
    "@angular/animations": "10.2.4",
    "@angular/cdk": "10.2.7",
    "@angular/common": "10.2.4",
    "@angular/compiler": "10.2.4",
    "@angular/core": "10.2.4",
    "@angular/fire": "6.0.4",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.2.4",
    "@angular/localize": "10.2.4",
    "@angular/material": "10.2.7",
    "@angular/platform-browser": "10.2.4",
    "@angular/platform-browser-dynamic": "10.2.4",
    "@angular/router": "10.2.4",
    "@angular/service-worker": "10.2.4",
    "@busacca/ng-pick-datetime": "10.0.0",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-build-classic": "27.0.0",
    "@ecodev/fab-speed-dial": "6.0.1",
    "@fortawesome/angular-fontawesome": "0.7.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "angular-calendar": "^0.28.22",
    "chart.js": "3.0.0-alpha",
    "core-js": "2.5.0",
    "date-fns": "^2.18.0",
    "eslint": "^7.19.0",
    "firebase": "^8.3.2",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "ng2-completer": "^9.0.1",
    "ng2-smart-table": "^1.7.2",
    "ngx-currency": "^2.5.1",
    "ngx-device-detector": "^2.0.6",
    "ngx-easy-table": "^13.0.3",
    "ngx-mask": "10.0.4",
    "ngx-mat-select-search": "3.2.0",
    "ngx-pagination": "5.0.0",
    "ngx-tour-core": "^4.2.0",
    "ngx-tour-md-menu": "^4.2.0",
    "rxjs": "6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.4",
    "@angular/language-service": "10.2.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@ngx-rocket/scripts": "3.0.3",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.132",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.1",
    "font-awesome": "^4.7.0",
    "hads": "^2.1.0",
    "htmlhint": "0.11.0",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-junit-reporter": "1.2.0",
    "protractor": "~7.0.0",
    "stylelint": "10.0.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.6.1",
    "ts-node": "8.2.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MidasHub/Midas_UI_Commercial.git"
  },
  "bugs": {
    "url": "https://github.com/MidasHub/Midas_UI_Commercial/issues"
